{
  "name": "@kristoferbaxter/async",
  "version": "0.0.7",
  "description": "Async Utilities for Module Node Development.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "author": "Kristofer Baxter",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "clean": "rimraf dist test/output",
    "format": "prettier --write '**/*.ts'",
    "prebuild": "yarn clean",
    "build": "tsc -p tsconfig.json",
    "postbuild": "node --experimental-modules build/build.mjs -p ./tsconfig.json --package",
    "build-test": "tsc -p test/tsconfig.json",
    "postbuild-test": "tsc-esm -p test/tsconfig.json",
    "pretest": "yarn build-test",
    "test": "node --experimental-modules test/output/test/tests.mjs",
    "release": "yarn build; yarn publish",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@kristoferbaxter/estree-walker": "2.0.2",
    "@types/node": "13.13.2",
    "acorn": "7.1.1",
    "acorn-import-meta": "1.0.0",
    "fast-glob": "3.2.2",
    "magic-string": "0.25.7",
    "mri": "1.1.5",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "typescript": "3.8.3",
    "typescript-esm": "1.0.1"
  },
  "volta": {
    "node": "14.0.0",
    "yarn": "1.22.4"
  },
  "prettier": {
    "printWidth": 150,
    "trailingComma": "all",
    "parser": "typescript",
    "singleQuote": true
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./pool": "./dist/pool.mjs"
  }
}